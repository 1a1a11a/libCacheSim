# - Config file for the libMimircache package
# It defines the following variables
#  LIBMIMIRCACHE_INCLUDE_DIR - include directory
#  LIBMIMIRCACHE_LIBRARIES    - libraries to link against

# Compute paths
get_filename_component(LIBMIMIRCACHE_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(LIBMIMIRCACHE_INCLUDE_DIR "@CONFIG_INCLUDE_DIRS@")

# Our library dependencies (contains definitions for IMPORTED targets)
include("${LIBMIMIRCACHE_CMAKE_DIR}/libMimircache-targets.cmake")

# These are IMPORTED targets created by libMimircache-targets.cmake
set(LIBMIMIRCACHE_LIBRARIES "@EXPORT_TARGETS@")